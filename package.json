{
	"name": "emblem-anims",
	"version": "1.0.0",
	"description": "",
	"main": "server.js",
	"scripts": {
		"build": "yarn clean && yarn build:types && yarn build:js",
		"build:client": "cd client && yarn build",
		"build:js": "babel server --out-dir build --extensions \".ts,.tsx\" --source-maps inline",
		"build:storybook": "cd client && build-storybook -s public",
		"build:types": "tsc",
		"clean": "rimraf build/*",
		"clean:node": "rimraf node_modules/*",
		"healthcheck": "yarn lint && yarn build",
		"lint": "yarn lint:js",
		"lint:js": "eslint server/**/*.ts --fix | yarn prettier --write",
		"prettier": "prettier \"**/*.{json,md,js,ts,tsx,jsx,css}\"",
		"start": "yarn lint && yarn build && node build/server.js",
		"start:client": "cd client && yarn start",
		"start:dev": "concurrently \"yarn start --ignore 'client/*'\" \"yarn start:client\"",
		"start:skip": "node build/server",
		"start:storybook": "cd client && yarn storybook",
		"test": "jest",
		"test:client": "cd client && yarn test"
	},
	"author": "Brady Shinners",
	"license": "ISC",
	"type": "module",
	"dependencies": {
		"body-parser": "^1.19.0",
		"bottleneck": "^2.19.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"googleapis": "47.0.0",
		"mongodb": "^3.5.4"
	},
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.8.7",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-proposal-numeric-separator": "^7.8.3",
		"@babel/plugin-proposal-object-rest-spread": "^7.8.3",
		"@babel/preset-env": "^7.8.7",
		"@babel/preset-typescript": "^7.8.3",
		"@types/bluebird": "^3.5.30",
		"@types/body-parser": "^1.19.0",
		"@types/enzyme": "^3.10.5",
		"@types/enzyme-adapter-react-16": "^1.0.6",
		"@types/express": "^4.17.3",
		"@types/jest": "24.9.0",
		"@types/mongodb": "^3.5.2",
		"@types/node": "^13.9.0",
		"@types/prop-types": "^15.7.3",
		"@types/react": "^16.9.23",
		"@types/react-dom": "^16.9.5",
		"@types/sequelize": "^4.28.8",
		"@types/storybook__react": "^5.2.1",
		"@types/validator": "^12.0.1",
		"@typescript-eslint/eslint-plugin": "^2.22.0",
		"@typescript-eslint/parser": "^2.22.0",
		"concurrently": "^5.1.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-jest": "^23.8.2",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
		"husky": "^4.2.3",
		"jest": "24.9.0",
		"lint-staged": "^10.0.8",
		"prettier": "1.19.1",
		"react": "^16.13.0",
		"rimraf": "^3.0.2",
		"typescript": "^3.8.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "yarn healthcheck"
		}
	},
	"resolutions": {
		"webpack": "4.41.5"
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,json,css,json,md}": [
			"prettier --write"
		],
		"server/**/*.{js,jsx,ts,tsx}": [
			"eslint --fix"
		]
	}
}
