{
    "compilerOptions": {
        "allowJs": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"jsx": "react",
		"moduleResolution": "node",
		"noImplicitReturns": true,
		"outDir": "./build",
		"resolveJsonModule": true,
		"strict": true,
		"target": "es2015",
	},
	"exclude": [
		"client",
		"build",
		"dist"
	]
}